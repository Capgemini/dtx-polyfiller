{
  "name": "DTX Polyfiller",
  "description": "Make DTX usable...",
  "version": "1.1",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["silhouette/silhouette.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [],
      "css": ["dtx.css"],
      "js": ["dtx.js"]
    },
    {
      "matches": [],
      "js": ["submit-claims/submit-claims-injector.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_popup": "options.html"
  },
  "web_accessible_resources": [
    "submit-claims/submit-claims-overrides.js"
  ]
}
